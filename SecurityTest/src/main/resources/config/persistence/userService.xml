<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.gnu.Mapper.UserMapper">
  <resultMap type="com.example.gnu.DTO.MyUser" id="MyUserResultMap">
	<id property="username" column="username"></id>
	  
  </resultMap>

  <select id="selectUserById" parameterType="map" resultType="String">
	select userid from userlist where userid = #{id} AND password = #{password}
  </select>
  <select id="findByUsername" resultMap="MyUserResultMap">
	select userid from userlist where userid = #{id} AND password = #{password}
  </select>
</mapper>